# BigQuery (dbt)
BIGQUERY_PROJECT_ID=your-gcp-project
BIGQUERY_DATASET_ID=analytics
BIGQUERY_LOCATION=US
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Meltano target-bigquery (z3z1ma)
TARGET_BIGQUERY_PROJECT=${BIGQUERY_PROJECT_ID}
TARGET_BIGQUERY_DATASET=${BIGQUERY_DATASET_ID}
TARGET_BIGQUERY_LOCATION=${BIGQUERY_LOCATION}
TARGET_BIGQUERY_CREDENTIALS_PATH=${GOOGLE_APPLICATION_CREDENTIALS}

# dbt sandbox user (used to name dev datasets)
DBT_USER=local

# tap-github
TAP_GITHUB_AUTH_TOKEN=ghp_xxx
